{
  "name": "furytigris",
  "version": "1.0.0",
  "description": "FuryTigris Launcher",
  "main": "main.js",
  "scripts": {
    "backend": "node server.js",
    "start": "electron-forge start",
    "make": "electron-forge make",
    "package": "electron-forge package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Organizacao-FuryTigris/FuryTigris-Source.git"
  },
  "author": "BielZcode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Organizacao-FuryTigris/FuryTigris-Source/issues"
  },
  "homepage": "https://github.com/Organizacao-FuryTigris/FuryTigris-Source#readme",
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^33.3.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2"
  },
  "electronForger": {
    "packagerConfig": {
      "icon": "./src/assets/icon.ico"
    },
    "makers": [
      {
        "name": "@electron-forge/maker-squirrel",
        "config": {
          "name": "furytigris_launcher"
        }
      },
      {
        "name": "@electron-forge/maker-zip",
        "platforms": [
          "win32",
          "win64"
        ]
      }
    ]
  }
}
